{
  "$schema": "http://json-schema.org/schema",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "palette": [
        {
          "slug": "a-white",
          "color": "rgb(255,255,255)",
          "name": "Acdev white"
        },
        {
          "slug": "a-gray-100",
          "color": "rgb(243 244 246)",
          "name": "Acdev Gray 100"
        },
        {
          "slug": "a-gray-200",
          "color": "rgb(229 231 235)",
          "name": "Acdev Gray 200"
        },
        {
          "slug": "a-gray-300",
          "color": "rgb(209 213 219)",
          "name": "Acdev Gray 300"
        },
        {
          "slug": "a-gray-400",
          "color": "rgb(156 163 175)",
          "name": "Acdev Gray 400"
        },
        {
          "slug": "a-gray-500",
          "color": "rgb(107 114 128)",
          "name": "Acdev Gray 500"
        },
        {
          "slug": "a-gray-600",
          "color": "rgb(75 85 99)",
          "name": "Acdev Gray 600"
        },
        {
          "slug": "a-gray-700",
          "color": "rgb(55 65 81)",
          "name": "Acdev Gray 700"
        },
        {
          "slug": "a-gray-800",
          "color": "rgb(31 41 55)",
          "name": "Acdev Gray 800"
        },
        {
          "slug": "a-gray-900",
          "color": "rgb(17 24 39)",
          "name": "Acdev Gray 900"
        },
        {
          "slug": "a-primary",
          "color": "rgb(239 68 68)",
          "name": "Acdev Primary"
        },
        {
          "slug": "a-accent",
          "color": "rgb(67 56 202)",
          "name": "Acdev Accent"
        },
        {
          "slug": "a-saccess",
          "color": "rgb(52 211 153)",
          "name": "Acdev Saccess"
        },
        { "slug": "a-info", "color": "rgb(96 165 250)", "name": "acdev Info" },
        {
          "slug": "a-warning",
          "color": "rgb(251 146 60)",
          "name": "Acdev Warning"
        },
        {
          "slug": "a-danger",
          "color": "rgb(236 72 153)",
          "name": "Acdev Danger"
        }
      ],
      "background": true,
      "text": true,
      "link": true,
      "custom": true,
      "duotone": [
        {
          "slug": "a-pink-sunset",
          "colors": ["#11245e", "#dc4379"],
          "name": "Acdev Pink Sunset"
        }
      ],
      "defaultGradients": false,
      "gradients": [
        {
          "slug": "a-summer-dog",
          "name": "Acdev Summer Dog",
          "gradient": "linear-gradient(#a8ff78, #78ffd6 )"
        }
      ]
    },
    "blocks": {
      "core/site-title": {
        "color": {
          "defaultPalette": true,
          "palette": [
            { "slug": "a-primary", "color": "#ef4444", "name": "Acdev Primary" }
          ],
          "link": true,
          "custom": false,
          "gradients": [],
          "customGradient": false
        },
        "typography": {
          "fontFamilies": [
            {
              "fontFamily": "Pacifico, sans-serif",
              "slug": "a-pacifico",
              "name": "Acdev Pacifico"
            }
          ]
        }
      },
      "core/cover": {
        "color": {
          "duotone": [],
          "customDuotone": false
        }
      },
      "core/preformatted": {
        "typography": {
          "fontSizes": [],
          "customFontSize": false
        }
      }
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Rubik, sans-serif",
          "slug": "a-rubik",
          "name": "Acdev Rubik"
        }
      ],
      "fontSizes": [
        { "slug": "small", "size": "0.75rem", "name": "Small" },
        { "slug": "medium", "size": "1.25rem", "name": "Medium" },
        { "slug": "large", "size": "2.25rem", "name": "Large" },
        { "slug": "x-large", "size": "3rem", "name": "Extra Large" },
        { "slug": "gigantic", "size": "3.75rem", "name": "Gigantic" }
      ],
      "lineHeight": true,
      "dropCap": true,
      "fontWeight": true,
      "fontStyle": true,
      "textTransform": true,
      "letterSpacing": true,
      "textDecoration": true
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1100px"
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "units": ["px", "rem", "vh", "vw", "%"],
      "blockGap": true
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--a-gray-700)",
      "background": "var(--wp--preset--color--a-white)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--a-primary)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "color": {
          "gradient": "var(--wp--preset--gradient--a-summer-dog)"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "2px",
          "radius": "10px",
          "style": "solid",
          "color": "var(--wp--preset--color--a-primary)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--a-pacifico)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--a-rubik)",
      "fontSize": "16px",
      "fontStyle": "normal",
      "fontWeight": "normal",
      "lineHeight": "inherit",
      "textDecoration": "none",
      "textTransform": "none"
    },
    "spacing": {
      "margin": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "padding": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "blockGap": "4rem"
    }
  }
}
